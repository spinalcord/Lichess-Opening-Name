{
    // Required
    "manifest_version": 3,
    "name": "Lichess Opening Names",
    "description": "Displays Opening Names durring a game",
    "version": "1.0.1",
    "icons":{
      "16":"images/16.png",
      "48":"images/48.png",
      "128":"images/128.png"
  },
    "content_scripts": [
        {
          "matches": ["https://lichess.org/*"],
          "js": ["main.js"]
        }
      ]
}